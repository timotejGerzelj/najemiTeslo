{
  "name": "lebocabinet",
  "keywords": [
    "11ty",
    "eleventy",
    "jamstack",
    "tailwindcss"
  ],
  "author": "Naomi Hauret",
  "license": "MIT",
  "version": "1.0.0",
  "description": "11ty + TailwindCSS barebones starter.",
  "scripts": {
    "setup": "npm install && npm run prepare",
    "prepare": "husky install",
    "clean": "rm -rf _site",
    "css:prettify": "prettier --parser=css --write \"./src/**/*.css\"",
    "js:prettify": "prettier --parser=babel --write \"./**/*.{js,ts}\"",
    "format": "npm run css:prettify && npm run js:prettify",
    "start": "NODE_ENV=development npm-run-all -l clean --parallel start:*",
    "start:webpack": "webpack --watch",
    "start:eleventy": "eleventy --serve",
    "build": "NODE_ENV=production npm-run-all -l clean build:*",
    "build:webpack": "webpack",
    "build:eleventy": "eleventy",
    "cm": "cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{json": [
      "prettier --write",
      "git add"
    ],
    "*.{css}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-add": "lint-staged"
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0-canary.23",
    "@babel/core": "^7.14.5",
    "babel-loader": "^8.2.2",
    "cheerio": "^1.0.0-rc.9",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "eleventy-plugin-prismic": "^0.0.2",
    "html-minifier": "^4.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-custom-media": "^7.0.8",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^5.3.0",
    "postcss-normalize": "^10.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "prismic-dom": "^2.2.5",
    "tailwindcss": "^2.1.4",
    "tailwindcss-logical": "^1.4.0",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-manifest-plugin": "^3.1.1"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.6",
    "toastify-js": "^1.11.1"
  }
}
